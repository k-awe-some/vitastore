{
  "name": "vitastore-server",
  "version": "1.0.0",
  "main": "app.js",
  "engines": {
    "node": "13.3.0",
    "yarn": "1.19.1"
  },
  "scripts": {
    "start": "node src/app.js",
    "start:server": "nodemon src/app.js",
    "start:client": "cd ../client && yarn start",
    "start:dev": "concurrently \"yarn start:server\" \"yarn start:client\""
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dialogflow": "^1.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  }
}
